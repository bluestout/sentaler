#loader-wrapper,
#fb-loading {
  .loader {
    width: 30px;
    height: 30px;

    span {
      position: absolute;
      top: 0;
      left: 0;
      display: block;
      width: 100%;
      height: 100%;

      &:before,
      &:after {
        position: absolute;
        width: 4px;
        height: 100%;
        background: #FFF;
        content: "";
      }

      &:before {
        top: auto;
        right: 0;
        bottom: 0;
        @include animation(drawframeRight 4s linear infinite 0.5s);
      }

      &:after {
        top: 0;
        bottom: auto;
        left: 0;
        @include animation(drawframeLeft 4s linear infinite 1.5s);
      }
    }

    &:before,
    &:after {
      position: absolute;
      width: 100%;
      height: 4px;
      background: #FFF;
      content: "";
    }

    &:before {
      top: 0;
      right: 0;
      left: auto;
      @include animation(drawframeTop 4s linear infinite);
    }

    &:after {
      right: auto;
      bottom: 0;
      left: 0;
      @include animation(drawframeBottom 4s linear infinite 1s);
    }
  }
}

