/*Phone & Tablet block*/
.product-item-wrapper {
  padding-left: $row_padding;
  padding-right: $row_padding;
  margin-bottom: 15px;
  .row-container {
    overflow: hidden;
    width: 100%;
    position: relative;
    background-color: transparent;
    .row-left {
      position: relative;
      overflow: hidden;
      .hoverBorderWrapper {
        .mask {
          display: block;
          float: left;
          width: 100%;
          height: 100%;
          position: absolute;
          top: 0;
          left: 0;
          background: #000;
          @include opacity(0,0);
          @include transition(ease, all, 0.5s);
        }
        img {
          margin: 0 auto;
        }
        .back {
          left: 0;
          opacity: 0;
          perspective: 500px;
          position: absolute!important;
          top: 0;
          @include transform_rotateY(-90deg);
          @include transform_preserve();
          @include transition(ease-in-out, all, 0.6s);
          visibility: hidden;
          width: 100%;
        }
      }
      .sale_banner {
        position: absolute;
        top: 0;
        left: 1px;
        margin-top: 10px;
      }
    }
    .row-right {
      text-align: left;
      position: relative;
      @include transition(ease, all, 0.5s);
      background-color: transparent;
      padding-bottom: 30px;
      .product-desc-list {
        display: none;
      }
      .product-price-list {
        display: none;
      }
      .avaibale-variants {
        margin-top: 14px;
        margin-bottom: 28px;
        display: none;
        .color-avai {
          img {
            border-radius: 50%;
            border: 1px solid rgba(0,0,0,0.1);
          }
        }
      }
      .hover-mask {
        text-align: center;
        width: 100%;
        z-index: 11;
        margin-top: 10px;
        @include transition(ease, all, 0.5s);
        @include opacity(1,100);
        @include transform_scale(1,1);
        .inner-mask {
          @include display_flex(center);
          width: 100%;
        }
        .inner-mask {
          form,
          ul.quickview-wishlist-wrapper {
            margin: 0;
            vertical-align: top;
          }
          form {
            width: 30px;
            margin-right: 2px;
            @media(max-width: 767px){
              width: 100%;
            }
            button.btn {
              width: 100%;
              height: $height_button_product;
              line-height: $height_button_product;
              background: $bkg_button;
              border: 1px solid $color_border_button;
              color: $color_text_button;
              padding: 0;
              @include border_radius(0);
              i.fa {
                margin-right: 5px;
                color: $color_text_button;
              }
              &:hover {
                background: $bkg_button_hover;
                border: 1px solid $color_border_button_hover;
                color: $color_text_button_hover;
                i.fa{
                  color: $color_text_button_hover;
                }
              }
            }
          }
          ul.quickview-wishlist-wrapper{
            @include display_flex(left);
            @media(max-width: 767px){
              display: none;
            }
            > li {
              list-style: none;
              margin: 0;
              cursor: pointer;
              vertical-align: top;
              &.quickview {
                margin-right: 2px;
                &:before {
                  position: absolute;
                  top: 0;
                  right: 0;
                  height: 40%;
                  background: #fff;
                  content: '';
                  width: 1px;
                  margin-top: 12px;
                }
              }
              &.wishlist{
                position: relative;
              }
              a {
                display: block;
                width: $height_button_product;
                height: $height_button_product;
                line-height: $height_button_product;
                @include border_radius(0);
                padding: 0;
                border: 1px solid $color_border_button;
                color: $color_text_button;
                background: $bkg_button;
                @include transition(ease, all, 0.5s);
                i.fa {
                  height: $height_button_product;
                  line-height: $height_button_product;
                  width: 20px;
                  text-align: center;
                  color: $color_text_button;
                }
              }
              div.quick_shop:hover > a, a.wish-list:hover {
                border-color: $color_border_button_hover;
                color: $color_text_button_hover;
                background: $bkg_button_hover;
                i.fa {
                  color: $color_text_button_hover;
                }
              }
              a.wish-list.wl-added {
                cursor: text;
                background: $bkg_button_hover;
                color: $color_text_button_hover;
                border-color: $color_border_button_hover;
                &:hover {
                  background: $bkg_button_hover;
                  color: $color_text_button_hover;
                  i.fa {
                    color: $color_text_button_hover;
                  }
                }
                i.fa {
                  color: $color_text_button_hover;
                  vertical-align: 1px;
                }
              }
              &.wishlist {
                form {
                  display: block;
                  line-height: 25px;
                  @include transition(ease, all, 0.5s);
                  .btn {
                    display: block;
                    height: $height_button_product;
                    line-height: $height_button_product;
                    border: none;
                    width: $height_button_product;
                    padding: 0;
                    color: $color_text_button;
                    border: 1px solid $color_border_button;
                    background: $bkg_button;
                    @include transition(ease, all, 0.5s);
                    i.fa {
                      height: $height_button_product;
                      line-height: $height_button_product;
                      width: $height_button_product;
                      text-align: center;
                      color: $color_text_button;
                      margin-right: 0;
                    }
                    &:hover {
                      color: $color_text_button_hover;
                      border-color: $color_border_button_hover;
                      background: $bkg_button_hover;
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
    &:hover {
      z-index: 1;
      .row-left {
        .hoverBorderWrapper {
          .back {
            opacity: 1;
            @include transform_rotateY(0deg);
            @include transition(ease-in-out, all, 0.4s);
            visibility: visible;
          }
        }
      }
      .row-right {
        .hover-mask {
          @include opacity(1,100);
          @include transform_scale(1,1);
        }
      }
    }
  }
}
@media (max-width: 1199px) {
  .product-item-wrapper {
    .row-container {
      &:hover {
        .hover-mask {
          @include transform_perspective(perspective(200px), rotate3d(1, 0, 0, 0deg), translateY(0));
