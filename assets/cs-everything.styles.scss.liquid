  &:hover {
    .row-left{
      .hoverBorder {
        .hoverBorderWrapper{
          span.back{
            @include transform_scale(1,1);
            @include opacity(1,100);
          }
        }
      }
      .hover-mask {
        @include transform_scale(1,1);
      }
    }
  }
}
/*End Tab products*/

/*Block video*/
$padding-video: 80px;
.video{
  background-attachment: fixed;
  .group_video {
    margin-top: $margin_distance;
    @media(max-width: 767px) {
      margin-top: $margin_distance_mobile;
