      &:after, &:before {
        content: '';
        position: absolute;
        width: 0;
        height: 0;
        border-color: transparent;
        @include transition(ease, all, 0.5s);
      }
      &:after {
        top: 0;
        left: 0;
        border-top-style: solid;
        border-top-width: 3px;
        border-left-style: solid;
        border-left-width: 3px;
      }
      &:before {
        bottom: 0;
        right: 0;
        border-bottom-style: solid;
        border-bottom-width: 3px;
        border-right-style: solid;
        border-right-width: 3px;
      }
      img{
        @include transition(ease, all, 0.5s);
      }
      &:hover {
        img{
          @include opacity(0.7,70);
          @include transform_scale(0.9,0.9);
          cursor: pointer;
        }
        &:after,
        &:before {
          width: 24px;
          height: 24px;
          border-color: $color_hover_general;
        }
      }
      .modal-content{
        background: transparent;
        border: none;
        box-shadow: none;
        .modal-header{
          display: none;
        }
      }
    }
  }
}
