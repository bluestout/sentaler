      margin-bottom: 2px;
      position: relative;
      a{
        color: #afafaf;
      }
    }
    .product-title{
      font-size:15px;
      color: $color_text_heading;
      @include transition(ease, all, 0.5s);
      font-weight: $font_weight_bold;
      text-overflow: ellipsis;
      overflow: hidden;
      a{
        color: $color_text_heading;
        font-size: 15px;
        white-space: normal;
        text-transform: capitalize;
            font-weight: normal;
      }
      &:hover {
        color: $color_hover_general;
      }
    }
    .group_price_rating{
      @include display_inline();
      width: 100%;
      margin-top: 5px;
      .rating-star {
        @include display_inline();
        float: right;
        margin-bottom: 0;
        @media(max-width: 600px) {
          display: block;
          float: none;
        }
        .spr-badge{
          .spr-badge-starrating {
            .spr-icon {
              font-size: 9px;
              color: $color_icon_spr;
              padding: 0 1px;
