            @include display_flex(right);
            float: right;
            position: relative;
            @media(max-width: 991px) {
              float: none;
              @include display_flex(center);
            }
            >li{
              padding: 0 10px;
              height: $height_header;
              &.login-account {
                position: relative;
              }
              i.fa{
                padding-right: 5px;
                font-size: $font-size_header;
                color: $color_text_banner;
              }
              &:last-child {
                padding-right: 0;
              }
              &.login-account {
                #loginBox {
                  min-width: 250px;
                  padding: 0;
                  #bodyBox{
                    @include display_inline();
                    >ul {
                      padding: 20px;
                      >li {
                        padding: 0;
                        width: 100%;
                        margin-bottom: 10px;
                        line-height: 1.55em;
                        &:last-child {
                          margin-bottom: 0;
                        }
                        a.btn, button {
                          color: $color_text_button1;
                          background: $bkg_button1;
                          border: 1px solid $color_border_button1;
                          height: $height_button;
                          line-height: $height_button;
                          text-transform: uppercase;
