        var connectedProductIdForBuilder = false;

        var analiseDataStructure = function (data) {
            if (typeof data !== "object") {
                return false;
            }

            if (!("handle" in data) || !("variants" in data) || !("title" in data)) {
                return false;
            }

            return true;
