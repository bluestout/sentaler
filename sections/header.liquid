{% if settings.tfcode == '' %}
<div class="emthemesModez-header-topSection">
  <div class="container" style="background:floralwhite; text-align:center; color: #000; font-size:11px; padding:5px 0; font-weight:400; letter-spacing:0; font-style:italic;box-shadow: 0 2px 5px rgba(0,0,0,0.2);border-bottom: 1px solid #999;text-transform: none;">
    Notice: Please enter your <strong>Item Purchase Code</strong> in <strong>Theme Customizer</strong> > <strong>Theme Settings</strong> tab > <strong>Item Purchase Code</strong> to activate your theme license and remove this notice.<img src="https://www.emthemes.com/media/shopify-logo-everything.png" alt="" />
  </div>
</div>
{% endif %}

<section class="top-header">
  <div class="top-header-wrapper">
    <div class="container">
      <div class="row">
        <div class="top-header-inner col-sm-12">
          <div class="col-sm-4">
          <div class="shipto">
                <!--<span class="ship_l">Ship to:</span>
            <span class="m_drop">
                <span class="ship_country">Czechia</span>
            	<span class="sticon-caret-d-small"></span>
            </span>-->
            <a href="tel:{{section.settings.loc_text | replace: ' ', ''}}"><i class="sticon-phone" aria-hidden="true"></i><span>{{section.settings.loc_text}}</span></a>
          </div>
          </div>
          <div class="col-sm-4 text-center">
          <div class="left-area">
            {% if section.settings.show_phone %}
            <div class="contact-info">{{ section.settings.phone_text }}</div>
            {% endif %}
          </div>
          </div>
          <div class="col-sm-4 text-right">
          <div class="right-area">
          {% if section.settings.show_location %}
            <!--<a href="tel:{{section.settings.loc_text | replace: ' ', ''}}"><i class="sticon-phone" aria-hidden="true"></i><span>{{section.settings.loc_text}}</span></a>-->
            <a href="{{section.settings.link_loc}}"><i class="sticon-location" aria-hidden="true"></i><span>{{section.settings.location}}</span></a>
            {% endif %}
           {% comment %}
            <ul class="unstyled customer_accounts">
              {% if section.settings.show_location %}
                <li class="store-location"><a href="{{ section.settings.loc_link}}"><i class="sticon-location" aria-hidden="true"></i> {{ section.settings.loc_text }}</a></li>
              {% endif %}
              <!-- Customer Links -->
              {% if shop.customer_accounts_enabled %}
                {% if customer  %}
                <li class="toolbar-customer my-account"><a href="/account"><i class="fa fa-user"></i> {{ 'layout.customer.your_account' | t }}</a></li>
                <li class="toolbar-customer log-out"><a href="/account/logout"><i class="fa fa-sign-out"></i> {{ 'layout.customer.logout' | t }}</a></li>
                {% else %}
                <li class="toolbar-customer login-account">
                  <span id="loginButton" class="dropdown-toggle" data-toggle="dropdown">
                    <i class="sub-dropdown1 visible-sm visible-md visible-lg"></i>
                    <i class="sub-dropdown visible-sm visible-md visible-lg"></i>
                    <i class="fa fa-unlock-alt"></i>
                    {{ 'sections.header.top_header_login' | t | customer_login_link }}
                  </span>
                  <div id="loginBox" class="dropdown-menu text-left">
                    {% form 'customer_login' %}
                    <div id="bodyBox">
                      <ul class="control-container customer-accounts list-unstyled">
                        <li class="clearfix">
                          <label for="customer_email_box" class="control-label">{{'layout.header.login_dropdown.customer_email'|t}} <span class="req">*</span></label>
                          <input type="email" value="" name="customer[email]" id="customer_email_box" class="form-control" />
                        </li>
                        {% if form.password_needed %}
                        <li class="clearfix">
                          <label for="customer_password_box" class="control-label">{{'layout.header.login_dropdown.customer_password'|t}} <span class="req">*</span></label>
                          <input type="password" value="" name="customer[password]" id="customer_password_box" class="form-control password" />
                        </li>
                        {% endif %}
                        <li class="clearfix">
                          <span class="required field">*{{'layout.header.login_dropdown.required'|t}}</span>
                        </li>
                        <li class="clearfix">
                          <a href="/account/login#recover"><span class="red">?</span> {{'layout.header.login_dropdown.forgot'|t}}</a>
                        </li>
                        <li class="clearfix">
                          <button class="btn btn-1" type="submit">{{'layout.header.login_dropdown.login'|t}}</button>
                        </li>
                        <li class="clearfix">
                          <a class="action btn btn-3" href="/account/register">{{'layout.header.login_dropdown.create'|t}}</a>
                        </li>
                      </ul>
                    </div>
                    {% endform %}
                  </div>
                </li>
                <li class="toolbar-customer create-account">
                  <i class="fa fa-key"></i>
                  {{ 'sections.header.top_header_register' | t | customer_register_link }}
                </li>
                {% endif %}
                <li class="toolbar-customer my-wishlist navUser-item--wishlist">
                  <a href="/pages/wish-list">
                    <i class="fa fa-heart"></i>
                    {{ 'layout.customer.wishlist' | t }}
                    <span class="wishlist-quantity"></span>
                  </a>
                </li>
                <!-- Currency -->
                {% if settings.show_multiple_currencies %}
                <li class="currency_group">
                  <div class="currencies-switcher">
                    {% include 'currencies-switcher' with settings.currency_style %}
                  </div>
                </li>
                {% endif %}
              {% endif %}
            </ul>
            {% endcomment %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
