<section class="main-header {% if template =='index' %}index{% elsif template contains 'header-v2' %}white fixed{% endif %}">
  <div class="main-header-wrapper">
    <div class="container">
      <div class="row">
        <div class="main-header-inner">
          <div data-section-id="{{ section.id }}" data-section-type="header-section">
            <div class="top_header_wrapper">
              <div class="top_header">

                <div class="middle-area col-sm-4">

                  <div class="desk_hide mobile_ham">
                    <span class="sticon-menu"></span> Menu
                  </div>

                  <nav class="site-nav small--hide" id="AccessibleNav" role="navigation">
                    {% include 'site-nav', nav_alignment: 'site-nav--centered', nav_link: section.settings.main_linklist %}
                  </nav>
                </div>
                <div class="left-area col-sm-4 text-center">
                  <div class="{{ logo_classes }}">
                    {% if section.settings.logo %}
                    <a href="/" itemprop="url" class="site-header__logo-image">
                      <span class="sticon-sentaler-full"></span>
                      <span class="sticon-sentaler" style="display:none"></span>
                    </a>

                    {% else %}
                    <a class="site-header__logo-link" href="/" itemprop="url">
                      <img src="{{ 'logo_200x.png' | asset_url }}" alt="" itemprop="logo">
                    </a>
                    {% endif %}
                  </div>
                </div>
                <div class="right-area col-sm-4">
                   <span class="sticon-search desk_hide" id="m_search"></span>
                    <div class="site-header__search m_hide">
                      {% include 'search-form' %}
                    </div>
                  {% if shop.customer_accounts_enabled %}
                  {% comment %}
                  <div class="customer_account m_hide">
      				        {% if customer  %}
                      <a href="/account" title="My Account">
                        <i class="sticon-user" aria-hidden="true"></i>
                      </a>
                    {% else %}
                      <a href="/account/login" title="Log in">
                        <i class="sticon-user" aria-hidden="true"></i>
                      </a>
                    {% endif %}
                  </div>
                  {% endcomment %}
                  {% endif %}
                  {% if shop.customer_accounts_enabled %}
                  <div class="wish-list-area border-it m_hide">
                    {% if customer  %}
                    <li class="toolbar-customer my-wishlist logged-wishlist" title="Wish List"><a href="/pages/wish-list"><i class="sticon-heart"></i></a></li>
                    {% else %}
                    <li class="toolbar-customer my-wishlist" title="Wish List"><a href="/pages/wish-list"><i class="sticon-heart"></i></a></li>
                    {% endif %}
                  </div>
                  {% endif %}
                  <div id="cart-target" class="toolbar-cart">
                    <a href="/cart" class="cart"  title="Shopping Cart">
                      <i class="sub-dropdown1 visible-sm visible-md visible-lg"></i>
                      <i class="sub-dropdown visible-sm visible-md visible-lg"></i>
                      <div class="num-items-in-cart">

                        <div class="block-cart"><i class="sticon-bag"></i>
                        {% if  cart.item_count > 0 %}
                         <div class="cart_count">{{ cart.item_count }}</div>
                        {% endif %}
                        </div>

                      </div>
                    </a>
                    <div id="cart-info" class="dropdown-menu">
                      <div id="cart-content">
                        <div class="loading">
                          <img src="{{'loader.gif' | asset_url}}" alt="" />
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="overlay_g"></div>
</section>

{% if section.settings.nav_sticky %}
<div class="fix-sticky"></div>
{% endif %}

<section class="mobile-nav-header">
  <div class="mobile-nav-header-wrapper">
    <div class="container">
      <div class="row">
        <div class="mobile-nav-header-inner">
          <div class="mobile-top-navigation visible-xs">
            <button id="showLeftPush" class="visible-xs"><i class="fa fa-bars fa-2x"></i></button>
            <ul class="list-inline">
              {% if shop.customer_accounts_enabled %}
              <li class="is-mobile-login">
                <div class="btn-group">
                  <div class="dropdown-toggle" data-toggle="dropdown">
                    <i class="fa fa-user"></i>
                  </div>
                  <ul class="customer dropdown-menu">
                    {% if customer %}
                    <li class="toolbar-customer log-out">
                      <a href="/account/logout">{{ 'layout.customer.logout' | t }}</a>
                    </li>
                    <li class="toolbar-customer my-account">
                      <a href="/account">{{ 'layout.customer.your_account' | t }}</a>
                    </li>
                    <li class="toolbar-customer my-wishlist">
                      <a href="/pages/wish-list">{{ 'layout.customer.wishlist' | t }}</a>
                    </li>
                    {% else %}
                    <li class="logout">
                      <a href="/account/login">{{'layout.header.login_dropdown.login'|t}}</a>
                    </li>
                    <li class="account">
                      <a href="/account/register">{{'layout.header.login_dropdown.create'|t}}</a>
                    </li>
                    {% endif %}
                  </ul>
                </div>
              </li>
              {% endif %}

              {% if settings.show_multiple_currencies %}
              <li class="is-mobile-currency currency_group">
                <div class="currencies-switcher">
                  {% include 'currencies-switcher' with settings.currency_style %}
                </div>
              </li>
              {% endif %}

              <li class="is-mobile-cart">
                <a href="/cart" title="Shopping Cart">
                  <div class="num-items-in-cart">
                    <i class="fa fa-shopping-cart"></i>
                      <span class="icon">
                        <span class="number">{{ cart.item_count }}</span>
                      </span>
                    <div class="ajax-subtotal" style="display:none;"></div>
                  </div>
                </a>
              </li>
            </ul>
          </div>

          <div class="mobile-search visible-xs">
            <div id="mobile-search-info">
              <form action="/search" method="get" class="search-header search" role="search">
                <div class="collections-selector">
                  <select class="single-option-selector" data-option="collection-option" id="collection-option" name="collection">
                    <option value="all">{{'sections.header.all_collection'|t}}</option>
                    {% for collection in collections %}
                    {% unless collection.all_products_count == 0 or collection.handle == "frontpage" %}
                    <option value="{{collection.handle}}">{{collection.title}}</option>
                    {% endunless %}
                    {% endfor %}
                  </select>
                </div>
                <input class="search-header__input search__input"
                       type="search"
                       name="q"
                       placeholder="{{ 'general.search.placeholder' | t }}"
                       aria-label="{{ 'general.search.placeholder' | t }}">
                <button class="search-header__submit search__submit btn--link" type="submit">
                  {% include 'icon-search' %}
                  <span class="icon__fallback-text">{{ 'general.search.submit' | t }}</span>
                </button>
              </form>
            </div>
          </div>

          <div class="is-mobile-nav-menu nav-menu visible-xs" id="is-mobile-nav-menu">
            <ul class="nav navbar-nav hoverMenuWrapper">
              {% for l in linklists[section.settings.main_linklist].links %}
              {% include 'header_navigation' %}
              {% endfor %}
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
{% include 'mobile-menu',nav_link: section.settings.main_linklist %}
{% if section.settings.nav_sticky %}
<script>
  function addaffix(scr){
    if(scr > 150){
        if(!$('.main-header').hasClass('baffix')){
          $('.main-header').addClass('affix').addClass('animated');
        }
      }
      else{
        if($('.main-header').hasClass('affix')){
          $('.main-header').removeClass('affix').removeClass('animated');
        }
      }


  }
  $(window).scroll(function() {
    var scrollTop = $(this).scrollTop();
    addaffix(scrollTop);
  });
  $( window ).resize(function() {
    var scrollTop = $(this).scrollTop();
	addaffix(scrollTop);
  });
</script>
{% endif %}


